[module/wlan]

type = internal/network
interface = wlp170s0

interval = 5.0

format-connected = <ramp-signal> <label-connected>
format-disconnected = <label-disconnected>

label-connected = %signal%%
label-disconnected = ${theme/icons.wireless-offline}

ramp-signal-font = 1

ramp-signal-0 = ${theme/icons.wireless-ok}
ramp-signal-1 = ${theme/icons.wireless-good}
ramp-signal-2 = ${theme/icons.wireless-excellent}

